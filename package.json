{
  "name": "cz-deploy",
  "version": "1.0.0-alpha.6",
  "description": "Creates a new version based on commit messages",
  "main": "lib/index.js",
  "bin": {
    "cz-add-tag": "bin/cz-add-tag.js",
    "cz-commit": "bin/cz-commit.js",
    "cz-last-tags-commits": "bin/cz-last-tag-commits.js",
    "cz-sync": "bin/cz-sync.js",
    "cz-update-version": "bin/cz-update-version.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "yarn release && node bin/cz-sync --push --push-tags",
    "release": "node bin/cz-update-version && node bin/cz-commit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacekkarczmarczyk/cz-deploy.git"
  },
  "keywords": [
    "commit",
    "build",
    "commitizen",
    "deploy",
    "javascript",
    "node",
    "nodejs",
    "git"
  ],
  "author": "Jacek Karczmarczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacekkarczmarczyk/cz-deploy/issues"
  },
  "homepage": "https://github.com/jacekkarczmarczyk/cz-deploy#readme",
  "dependencies": {
    "semver": "^7.1.1",
    "simple-git": "^1.129.0",
    "yargs": "^15.0.2"
  }
}
