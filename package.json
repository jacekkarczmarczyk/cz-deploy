{
  "name": "cz-deploy",
  "version": "1.0.0-alpha.1",
  "description": "Creates a new version based on commit messages",
  "main": "lib/index.js",
  "bin": {
    "cz-add-tag": "bin/cz-add-tag.js",
    "cz-commit": "bin/cz-commit.js",
    "cz-update-version": "bin/cz-update-version.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "cz-release && yarn merge",
    "merge": "git checkout master && git merge dev && git push && git checkout dev && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacekkarczmarczyk/cz-deploy.git"
  },
  "keywords": [
    "commit",
    "build",
    "commitizen",
    "deploy",
    "javascript",
    "node",
    "nodejs",
    "git"
  ],
  "author": "Jacek Karczmarczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacekkarczmarczyk/cz-deploy/issues"
  },
  "homepage": "https://github.com/jacekkarczmarczyk/cz-deploy#readme",
  "dependencies": {
    "semver": "^5.6.0",
    "simple-git": "^1.107.0",
    "yargs": "^13.2.2"
  }
}
